{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "gdb",
          "request": "attach",
          "name": "gdb remote debug",
          "gdbpath": "/usr/bin/gdb-multiarch",
          "executable": "./a.out",
          "target": ":1234",
          "remote": true,
          "printCalls": true,
          "cwd": "${workspaceRoot}",
          "valuesFormatting": "parseText"
      }
  ]
}